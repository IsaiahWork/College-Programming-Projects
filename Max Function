#include <iostream>

using namespace std;



int main(){

int num1, num2;

cout << "Enter the first number: ";
cin >> num1;
cout<< "Enter the second number: ";
cin >> num2;

if(num1 > num2){
    cout<< "The first Number is Larger: " <<num1;
    cout<<" The second number is Smaller: " << num2;
}else{
    cout<<"The second Number is Larger: "<<num2;
    cout<<" The first number is smaller: "<<num1;

    return 0;
}
}
